{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "src",
            "public"
        ],
        "excludes": [
            "Migrations"
        ]
    },
    "mutators": {
        "@default": true,
        "LogicalAndAllSubExprNegation": {
            ignore: [
                "Ramona\\Ras2\\Serialization\\Normalizer"
            ]
        }
    },
    "logs": {
        "text": "infection.log"
    }
}